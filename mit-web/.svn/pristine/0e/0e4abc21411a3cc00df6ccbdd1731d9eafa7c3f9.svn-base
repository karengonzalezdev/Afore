#leftMainMenu {
    margin: 20px 0;
}

/* General align styles begin */
.toCenter, .textPercent {
  text-align: center;
}

.toRight {
  text-align: right;
}

.textNumeric, .textMoney {
  text-align: right;
  white-space: nowrap;
}

.toLeft {
  text-align: left;
}
